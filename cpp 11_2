//Практика 11 задание 2
#include <iostream>
#include <deque>
#include <string>
int main() {
    std::deque<std::string> orders;
    orders.push_front( "1 urgent order");
    orders.push_front( "2 urgent order");
    orders.push_back("1 order");
    orders.push_back("2 order");

    std::cout << "Queue of the orders: " << std::endl;
    for (const auto& order : orders ) {
        std::cout << order << std::endl;
    }
    while (!orders.empty()) {
        std::cout << "Prosessing the orders: " << orders.front() << std::endl;
        orders.pop_front();
    }

    std::cout << "\n Orders are empty" << std::endl;
}